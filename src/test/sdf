# TEST: sdf.0
set -eu
S=`pwd` U=u/sdf C=conf/default.h
u.conf "$S" "$U" "$C" <<!
run
!
(u.make clean && u.make -j) > /dev/null
cp "$S"/data/sdf/cyl1/cyl.dat  sdf.dat
for x in 0.0 3.0 6.0 9.0
do
   u.run ./udx $x 0 0
done > sdf.out.txt

# TEST: sdf.1
set -eu
S=`pwd` U=u/sdf C=conf/default.h
u.conf "$S" "$U" "$C" <<!
field_dumps
run
!
(u.make clean && u.make -j) > /dev/null
cp "$S"/data/sdf/cyl1/cyl.dat  sdf.dat
mkdir -p h5
u.run ./udx 0 0 0
u.wall.dump h5/wall.h5     > wall.out.txt
