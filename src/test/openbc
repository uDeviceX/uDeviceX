#### rest with deleting cylinder
# nTEST: out.t1
:
u.conf conf/default.h <<!!!
   tend=2.01 part_freq=4000
   field_dumps part_dumps
   field_freq=4000
   OUTFLOW OUTFLOW_CIRCLE OUTFLOW_CIRCLE_R=8
   FORCE_PAR_A=0
   run
!!!
(make clean && u.make -j) > /dev/null
./udx
u.avgh5 0 u 0 h5/flowfields-0001.h5 | uscale 10 > h5.out.txt

#### rest with deleting cylinder, multi nodes
# nTEST: out.t2
:
u.conf conf/default.h <<!!!
   tend=2.01 part_freq=4000
   field_dumps part_dumps
   field_freq=4000
   OUTFLOW OUTFLOW_CIRCLE OUTFLOW_CIRCLE_R=8
   FORCE_PAR_A=0
   XS=8 YS=8
   run
!!!
(make clean && u.make -j) > /dev/null
u.run 2 2 1 ./udx
u.avgh5 0 u 0 h5/flowfields-0001.h5 | uscale 10 > h5.out.txt

#### rest with deleting cylinder and cylinder inflow 
# nTEST: inout.t1
u.conf conf/default.h <<!!!
   tend=2.01 part_freq=4000
   field_dumps part_dumps
   field_freq=4000
   OUTFLOW OUTFLOW_CIRCLE OUTFLOW_CIRCLE_R=8
   INFLOW FORCE_PAR_A=0
   INFLOW_CIRCLE_R=1
   INFLOW_CIRCLE_H=16
   INFLOW_CIRCLE_U=1
   INFLOW_CIRCLE_OX=XS/2
   INFLOW_CIRCLE_OY=YS/2
   INFLOW_CIRCLE_OZ=ZS/2
   run
!!!
(make clean && u.make -j) > /dev/null
./udx
u.avgh5 0 u 0 h5/flowfields-0001.h5 | uscale 10 > h5.out.txt
