#### spheres in double poiseuille, no contact
# nTEST: multisolid.t1
# export PATH=../tools:$PATH
# (cd ../cmd; make ) > /dev/null
# rm -rf bop h5 diag.txt solid_diag.txt rbc.off
# echo -e "4.1 21.5 8\n4.1 10.5 8\n11.9 16 8" > ic_solid.txt
# cp data/rig/sphere.ply   mesh_solid.ply
# cp data/cells/rbc.498.off rbc.off
# :
# u.conf conf/double.poiseuille.h <<!!!
#   solids sbounce_back tend=0.5 part_freq=100
#   pushflow doublepoiseuille field_dumps part_dumps
#   wall_creation=0 contactforces=false field_freq=100
#   run
#!!!
# (make clean && u.make -j && make -C ../tools) > /dev/null
# sh runfile
# bop2test.py bop/solid-00004.bop | awk '{print $1, $2}' | uscale 10 > bop.out.txt
#

#### spheres in double poiseuille, contact
# nTEST: multisolid.t2
# export PATH=../tools:$PATH
# (cd ../cmd; make ) > /dev/null
# rm -rf bop h5 diag.txt solid_diag.txt
# echo -e "4.1 21.5 8\n4.1 10.5 8\n11.9 16 8" > ic_solid.txt
# cp data/rig/sphere.ply mesh_solid.ply
# :
# u.conf conf/double.poiseuille.h <<!!!
#   solids -sbounce_back tend=0.5 -part_freq=100
#   pushflow -doublepoiseuille field_dumps -part_dumps
#   wall_creation=0 contactforces=true field_freq=100
#   run
#!!!
# (make clean && u.make -j && make -C ../tools) > /dev/null
# sh runfile
# bop2test.py bop/solid-00004.bop | awk '{print $1, $2}' | uscale 10 > bop.out.txt
#

#### ellipsoids in double poiseuille, contact
